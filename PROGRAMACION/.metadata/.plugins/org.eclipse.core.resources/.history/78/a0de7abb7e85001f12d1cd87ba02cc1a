package Ejercicio2;

public class Ejercicio19 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int lastNumber = 1; 
		
		for (int i = 1; i <=9; i++) {
			System.out.println("");
			for (int x = 0; x <= lastNumber; x++) {
				System.out.print(x + "");
				lastNumber++; 
			}
		}
	}

}z
