package Ejercicio2;

import java.util.Scanner;

public class Ejercicio9 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sca = new Scanner(System.in); 
		
		System.out.println("Indique su nombre completo");
		
		String nombreCompleto = sca.nextLine(); 
		
		
		System.out.println("Su nombre es " + nombreCompleto.substring(0,5));
		System.out.println("Su primer apellido es " + nombreCompleto.substring(8,12));
		System.out.println("Su segundo apellido es " + nombreCompleto.substring(12,20));
		
		
		sca.close();

	}

}
