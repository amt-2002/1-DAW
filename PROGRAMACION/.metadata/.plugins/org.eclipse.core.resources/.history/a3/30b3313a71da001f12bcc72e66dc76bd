package Tema4.Ej33;



public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Semaforo semaforo1 = new Semaforo();
		
		semaforo1.setColor("AZUL");
		
		System.out.println(semaforo1.getColor());
		
		semaforo1.setColor("VERDE");
		
		semaforo1.setParpadeando(true);
		
		semaforo1.setColor("AMBAR");
		
		
		for (int i = 1; i <= 5; i++) {
			semaforo1.cambiarEstado();
			System.out.println(semaforo1.toString());
		}
		
		
		Semaforo semaforo2 = semaforo1; 
		
		System.out.println(semaforo2.toString());
	}

}
