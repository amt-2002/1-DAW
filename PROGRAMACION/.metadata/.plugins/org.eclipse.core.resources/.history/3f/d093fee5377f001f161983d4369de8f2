package Ejercicio2;

import java.util.Scanner;

public class Ejercicio5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		int numeros = 0; 
		
		while (numeros <= 10000) {
			numeros = numeros + 1;
			System.out.println("La cantidad sumada es " + numeros);
		}
		
	}

}
