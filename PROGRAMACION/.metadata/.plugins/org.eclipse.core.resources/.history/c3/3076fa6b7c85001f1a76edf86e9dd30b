package Ejercicio2;

public class Ejercicio18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int contador = 0; 
		do {
			contador++; 
			for (int i = 1; i<=5; i++) {
				System.out.print(i + " ");
			}
			
			System.out.println("");
			
		} while(contador<=5); 
	}

}
